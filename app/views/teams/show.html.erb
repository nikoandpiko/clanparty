                </div>
                <br>
                <br>
                <br>
                <br>
                <% if event.day == 1 %>
                  <h4 class="card-day"> <i class="far fa-clock"></i> Sunday</h4>
                <% elsif event.day == 2 %>
                  <h4 class="card-day"> <i class="far fa-clock"></i> Monday</h4>
                <% elsif event.day == 3 %>
                  <h4 class="card-day"> <i class="far fa-clock"></i> Tuesday</h4>
                <% elsif event.day == 4 %>
                  <h4 class="card-day"> <i class="far fa-clock"></i> Wedesday</h4>
                <% elsif event.day == 5 %>
                  <h4 class="card-day"> <i class="far fa-clock"></i> Thursday</h4>
                <% elsif event.day == 6 %>
                  <h4 class="card-day"> <i class="far fa-clock"></i> Friday</h4>
                <% elsif event.day == 7 %>
                  <h4 class="card-day"> <i class="far fa-clock"></i> Saturday</h4>
                <% end %>
                <h3 class="card-secondary-text pt-2"> <%= event.start_time.strftime("%h 4") %>, <%= event.start_time.strftime("%I:%M") %> - <%= event.end_time.strftime("%I:%M") %> JST</h3>
                <%# Name and Description are reversed until seed is updated %>
