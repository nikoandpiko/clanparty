<div class="container">
  <div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(https://p82.f1.n0.cdn.getcloudapp.com/items/YEuXB4Pj/725a5ca9-290c-406a-99ce-3f3aea2529ae.png?source=viewer&v=733bc8b84c6772f52aaef17de2a7035f);">
    <div class="container">
      <div class="card-product p-1 text-left" style="background-color:transparent;">
        <img src="<%= @team.avatar %>" alt="<%= @team.name %>" style="margin-top:-30px;border-radius:8px;">
        <div style="margin-left: 16px">
          <h2><%= @team.game.title %></h2>
          <h1><%= @team.name %></h1>
          <p><%= @team.bio %></p>
          <%# <h5 class="text-white">(Core Hours HERE)</h5> %>
        </div>
      </div>
    </div>
  </div>

  <div class="modal fade" id="eventModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLongTitle">Enter the Event Details</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body no-padding">
          <%= simple_form_for [ @team, @event ] do |f| %>
          <%= f.input :name, label: "Event", collection: ["The Sirensong Sea", "Amaurot", "The Dying Gasp", "Anamnesis Anyder", "Mt. Gulg"] %>
          <%= f.input :description, placeholder: "This is a weekly event to ensure our group gets the best gear." %>
          <%= f.input :day, collection: ["Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday"] %>
          <%= f.input :start_time %>
          <%= f.input :end_time %>
            <div class="modal-footer p-0">
              <%= f.button :submit, "Create Event", id:"sweet-alert-event", class: "btn btn-dark btn-lg btn-block m-0 mb-2" %>
              <button type="button" class="btn btn-secondary btn-lg btn-block m-0" data-dismiss="modal">Go Back</button>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>

